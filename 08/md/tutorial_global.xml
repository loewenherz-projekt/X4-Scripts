          </actions>
        </cue>

        <cue name="FirstPickupWareHint">
          <conditions>
            <event_player_collected_ware/>
            <check_value value="not event.param.isinventory" />
          </conditions>
          <actions>
            <show_help line="3341" position="18" force="true" comment="Inventory items are small things you carry with you whereas cargo is stored in your ships." log="true"/>
          </actions>
        </cue>

        <cue name="FirstPickupItemHint">
          <conditions>
            <event_inventory_added object="player.entity"/>
            <check_value value="player.age gt 30s"/>
            <check_any exact="event.param.keys.count" counter="$i">
              <check_value value="not event.param.keys.{$i}.ispaintmod" />
            </check_any>
          </conditions>
          <actions>
            <show_help line="3341" position="18" force="true" comment="Inventory items are small things you carry with you whereas cargo is stored in your ships." log="true"/>
          </actions>
        </cue>


        <cue name="FirstHireCrewHint">
          <conditions>
            <event_ui_triggered screen="'MapMenu'" control="'menu_hiremode'"/>
          </conditions>
          <actions>
            <show_help line="if player.input.controller then 17122 else 17120" position="1" force="true" comment="RIGHT CLICK the ship you want to assign. / Press $INPUT_STATE_WIDGET_INTERACT$ on the ship you want to assign."/>
            <show_help line="17130" position="1" force="true" comment="Select a crew category to assign and CONFIRM."/>
          </actions>
          <cues>
            <cue name="FirstHireCrewHint_DialogClosed">
              <conditions>
                <event_ui_triggered screen="'MapMenu'" control="'menu_close'"/>
              </conditions>
              <actions>
                <remove_help line="17130" />
                <remove_help line="17120" />
                <remove_help line="17122" />
              </actions>
            </cue>
          </cues>
        </cue>

        <cue name="FirstEmergencyEject">
          <conditions>
            <event_player_ejected />
          </conditions>
          <actions>
            <show_help line="917" position="1" duration="18s" force="true" comment="Your ship has been destroyed, Emergency Eject been activated, and you are now in your spacesuit. If you cannot reach a station, and don't have another ship to come and rescue you before your oxygen runs out, then your options may be very limited."/>
          </actions>
        </cue>

        <cue name="WeaponSlotEmptyHint">
          <conditions>
            <event_player_fired_empty_weapon_group/>
          </conditions>
          <actions>
            <set_value name="$FireEmptyTime" exact="player.age"/>
          </actions>
          <cues>
            <cue name="WeaponSlotEmptyTimeout">
              <delay exact="2s"/>
              <actions>
                <reset_cue cue="WeaponSlotEmptyHint"/>
              </actions>
            </cue>
            <cue name="WeaponSlotEmptyReset" comment="cooldown to avoid spamming this hint">
              <delay exact="120s"/>
              <actions>
                <reset_cue cue="WeaponSlotEmptyHint"/>
              </actions>
            </cue>
            <cue name="WeaponSlotEmpty2Hint" comment="tried to fire empty weapongroup multiple times, within a short interval">
              <conditions>
                <event_player_fired_empty_weapon_group/>
              </conditions>
              <actions>
                <do_if value="player.age lt $FireEmptyTime + 1.0">
                  <show_help line="923" position="18" duration="7s" width="220" force="true" comment="." halign="'center'"/>
                  <cancel_cue cue="WeaponSlotEmptyTimeout"/>
                </do_if>
              </actions>
            </cue>
          </cues>
        </cue>

        <cue name="UseCounterMeasures">
          <conditions>
            <event_object_incoming_missile group="global.$PlayerOccupiedShipGroup" />
            <check_value value="player.ship.ammostorage.countermeasure.count"/>
          </conditions>
          <delay exact="2s" comment="betty warning 'incoming missile, so spread the hint out to not spam the player with info"/>
          <actions>
            <show_help line="916" position="1" duration="7s" width="220" force="true" comment="." halign="'center'"/>
            <cancel_cue cue="BuyCounterMeasures" comment="player already purchased, otherwise he couldn't have launched"/>
          </actions>
        </cue>

        <cue name="BuyCounterMeasures">
          <conditions>
            <event_object_incoming_missile group="global.$PlayerOccupiedShipGroup" />
            <check_value value="not player.ship.ammostorage.countermeasure.count"/>
          </conditions>
          <delay exact="2s" comment="betty warning 'incoming missile, so spread the hint out to not spam the player with info"/>
          <actions>
            <show_help line="915" position="1" duration="7s" width="220" force="true" comment="." halign="'center'"/>
          </actions>
        </cue>

        <!-- Cover ability tutorial starts when player sits down in the pilot chair of a ship with cover ability for the first time. Replays for each individual ship because the ability is so rare. -->
        <cue name="UseCoverAbility">
          <conditions>
            <!-- The cover button only appears when undocked, so we don't want the tutorial to appear when docked. -->
            <check_any>
              <check_all>
                <event_player_started_control/>
                <check_value value="player.entity.$x4ep1_player_cover_ability.{event.param}?"/>
                <check_value value="not @$CoverAbilityTutorialCheckedOffShips.indexof.{event.param}"/>
                <check_value value="not event.param.dock"/>
              </check_all>
              <check_all>
                <event_object_undocked group="global.$PlayerControlledGroup"/>
                <check_value value="player.entity.$x4ep1_player_cover_ability.{event.object}?"/>
                <check_value value="not @$CoverAbilityTutorialCheckedOffShips.indexof.{event.object}"/>
                <check_value value="event.object.pilot == player.entity"/>
              </check_all>
              <event_cue_signalled comment="For forcing a repeat of the tutorial. Pass in ship as event.param!"/>
            </check_any>
          </conditions>
          <actions>
            <do_if value="event.name == 'event_object_undocked'">
              <set_value name="this.$ship" exact="event.object"/>
            </do_if>
            <do_else comment="event_player_started_control and event_cue_signalled set event.param">
              <set_value name="this.$ship" exact="event.param"/>
            </do_else>
            <do_if value="not $CoverAbilityTutorialCheckedOffShips?">
              <create_group groupname="$CoverAbilityTutorialCheckedOffShips"/>
            </do_if>
            <add_to_group groupname="$CoverAbilityTutorialCheckedOffShips" object="this.$ship"/>
            <show_help line="937" position="1" force="true" allowclose="false" duration="30s" timeout="true" log="true" comment="This hint is too easy to accidentally close with allowclose = true"/>
          </actions>
          <cues>

            <cue name="UseCoverAbility_ShipInteractionMenuOpened">
              <conditions>
                <event_ui_triggered screen="'DockedMenu'"/>
              </conditions>
              <actions>
                <show_help_overlay id="'docked_cover'" highlightonly="true"/>
                <remove_help line="937"/>
                <show_help line="938" position="18" force="true" allowclose="true" timeout="false" log="true"/>
              </actions>
              <cues>

                <cue name="UseCoverAbility_CoverEnabled">
                  <conditions>
                    <event_ui_triggered screen="'DockedMenu'" control="'docked_cover'"/>
                  </conditions>
                  <cues>

                    <cue name="UseCoverAbility_Disabled">
                      <conditions>
                        <event_ui_triggered screen="'DockedMenu'" control="'docked_cover'"/>
                      </conditions>
                      <actions>
                        <remove_help line="938"/>
                        <signal_cue cue="UseCoverAbility_RemoveHint"/>
                      </actions>
                    </cue>

                  </cues>
                </cue>

              </cues>
            </cue>

            <cue name="UseCoverAbility_RemoveHint">
              <conditions>
                <check_any>
                  <event_ui_triggered screen="'hintclosed'" control="'938'"/>
                  <event_cue_signalled/>
                </check_any>
              </conditions>
              <actions>
                <remove_help_overlay id="'docked_cover'"/>
                <reset_cue cue="parent"/>
              </actions>
            </cue>

            <cue name="UseCoverAbility_ResetTutorial">
              <conditions>
                <event_entity_left space="parent.$ship"/>
                <check_value value="event.param == player.entity"/>
              </conditions>
              <actions>
                <!-- Renmove help in case the player leaves the ship before the tutorial is complete. -->
                <remove_help_overlay id="'docked_cover'"/>
                <remove_help line="937"/>
                <remove_help line="938"/>
                <reset_cue cue="parent"/>
              </actions>
            </cue>

          </cues>
        </cue>


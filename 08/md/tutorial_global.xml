        <cue name="IllegalStationHint" instantiate="true">
          <conditions>
            <event_ui_triggered screen="'MapMenu'" control="'initiateconstruction_license'"/>
            <check_value value="event.param3 == 0"/>
          </conditions>
          <actions>

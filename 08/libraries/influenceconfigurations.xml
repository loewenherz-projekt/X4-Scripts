<?xml version="1.0" encoding="utf-8"?>
<influencelistconfigurations xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:xsd="http://www.w3.org/2001/XMLSchema" xsi:noNamespaceSchemaLocation="influenceconfigurations.xsd">
  <!--Other influences-->

  <influencelist id="emp_missile" name="{20230,2001}" description="{20230,2002}">
    <influences>
      <influence type="disableboost">
        <profile type="continuous" duration="30" />
      </influence>
      <influence type="disabletravel">
        <profile type="continuous" duration="30" />
      </influence>
    </influences>
  </influencelist>

  <influencelist id="region_travel_disrupt" name="{20230,2005}" description="{20230,2006}">
    <influences classes="ship">
      <influence type="disabletravel">
        <profile type="continuous" duration="10" />
      </influence>
    </influences>
  </influencelist>

  <influencelist id="recharge_oxygen">
    <influences classes="spacesuit">
      <influence type="chargeoxygen">
        <profile type="continuous" startvalue="25" duration="1" />
      </influence>
    </influences>
  </influencelist>

  <influencelist id="damage_radiation_slow">
    <influences>
      <influence type="hullshielddamage">
        <profile type="continuous" startvalue="0.01" duration="1" />
      </influence>
    </influences>
  </influencelist>

  <influencelist id="test_speed">
    <influences classes="ship">
      <influence type="disableboost">
        <profile type="continuous" duration="5"/>
      </influence>
      <influence type="maxforwardspeed">
        <profile type="envelope" delay="3" sustainvalue="0.0" sustainduration="2"/>
      </influence>
      <influence type="maxreversespeed">
        <profile type="envelope" delay="3" sustainvalue="0.0" sustainduration="2"/>
      </influence>
    </influences>
  </influencelist>

  <influencelist id="test_accel">
    <influences classes="ship">
      <influence type="disableboost">
        <profile type="continuous" duration="8"/>
      </influence>
      <influence type="forwardacceleration">
        <profile type="envelope" sustainvalue="0.0" sustainduration="8"/>
      </influence>
      <influence type="reverseacceleration">
        <profile type="envelope" sustainvalue="0.0" sustainduration="8"/>
      </influence>
    </influences>
  </influencelist>

  <influencelist id="test_steering">
    <influences classes="ship">
      <!--<influence type="steeringacceleration">-->
      <influence type="maxsteeringspeed">
        <profile type="envelope" sustainvalue="0.0" sustainduration="5"/>
      </influence>
    </influences>
  </influencelist>


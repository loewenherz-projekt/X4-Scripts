          <splineposition x="15000" z="25000" weight="1" />
          <splineposition x="0" z="25000" weight="0" />
          <size r="1000" />
        </boundary>
      </boundaries>
      <controls>
        <linear>
          <time min="20" max="30" />
        </linear>
        <angular>
          <roll max="25" />

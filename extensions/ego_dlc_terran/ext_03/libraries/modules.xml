        <limits production="2"/>
        <maxlimits production="6"/>
        <production ware="terranmre" chance="65" />
        <production ware="proteinpaste" chance="10" />
      </compatibilities>
    </module>

    <module id="prod_ter_siliconcarbide" group="prod_ter_siliconcarbide">
      <category ware="siliconcarbide" tags="[production, module]" race="terran" faction="[pioneers, terran]" />
      <compatibilities>
        <limits production="3"/>
        <maxlimits production="9"/>
        <production ware="siliconcarbide" chance="50" />
      </compatibilities>
    </module>

    <module id="prod_ter_stimulants" group="prod_ter_stimulants">
      <category ware="stimulants" tags="[production, module]" race="terran" faction="[pioneers, terran]" />

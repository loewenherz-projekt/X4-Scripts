<?xml version="1.0" encoding="utf-8"?>
<diff xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:noNamespaceSchemaLocation="diff.xsd">
  <add sel="/modules">
    <!-- Terran Production Modules -->

    <module id="prod_ter_computronicsubstrate" group="prod_ter_computronicsubstrate">
      <category ware="computronicsubstrate" tags="[production, module]" race="terran" faction="[pioneers, terran]" />
      <compatibilities>
        <limits production="1"/>
        <maxlimits production="3"/>
        <production ware="computronicsubstrate" chance="20" />
      </compatibilities>
    </module>

    <module id="prod_ter_energycells" group="prod_ter_energycells" base="ter_solarpowerplant_2">
      <category ware="energycells" tags="[production, module]" race="terran" faction="[pioneers, terran]" />
      <compatibilities>
        <limits production="24"/>
        <production ware="energycells" chance="75" />
      </compatibilities>
    </module>

    <module id="prod_ter_medicalsupplies" group="prod_ter_medicalsupplies">
      <category ware="medicalsupplies" tags="[production, module]" race="terran" faction="[pioneers, terran]" />
      <compatibilities>
        <production ware="medicalsupplies" chance="65" />
      </compatibilities>
    </module>

    <module id="prod_ter_metallicmicrolattice" group="prod_ter_metallicmicrolattice">
      <category ware="metallicmicrolattice" tags="[production, module]" race="terran" faction="[pioneers, terran]" />
      <compatibilities>
        <production ware="metallicmicrolattice" chance="65" />
      </compatibilities>
    </module>

    <module id="prod_ter_proteinpaste" group="prod_ter_proteinpaste">
      <category ware="proteinpaste" tags="[production, module]" race="terran" faction="[pioneers, terran]" />
      <compatibilities>
        <limits production="2"/>
        <maxlimits production="6"/>
        <production ware="proteinpaste" chance="65" />
        <production ware="medicalsupplies" chance="25" />
        <production ware="terranmre" chance="10" />

    <!-- Terran Dockarea Modules -->
    <module id="dockarea_ter" group="dockarea_ter">
      <category tags="[dockarea, module, lowtech, standard, hightech]" race="terran" faction="[pioneers, terran]" />
    </module>

    <!-- Terran Pier Modules -->
    <module id="pier_base_ter" group="pier_base_ter">
      <category tags="[pier, base, module]" race="terran" faction="[pioneers, terran]" />
    </module>

    <module id="pier_add_ter" group="pier_add_ter">
